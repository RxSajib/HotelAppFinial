<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/LandingDrawerID"
    tools:context=".Landing_Activity">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <ImageView
        android:id="@+id/LaveSpaID"
        android:layout_above="@+id/Twos"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:src="@drawable/lavespaicon">

    </ImageView>


    <ImageView
        android:id="@+id/NavImageID"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/menumenu">

    </ImageView>


    <Button
        android:id="@+id/JoinButtonID"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/join_design"
        android:text="Join"
        android:textColor="@android:color/black">

    </Button>


    <Button
        android:id="@+id/SiginButtonID"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/JoinButtonID"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/sigin_design"
        android:text="Sigin"
        android:textColor="@android:color/white"></Button>


    <RelativeLayout

        android:id="@+id/KKK"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_above="@+id/ButtonID"
        android:layout_marginBottom="5dp">


        <LinearLayout
            android:id="@+id/LayoutOne"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="3">


            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="3dp"
                android:elevation="5dp"
                android:layout_height="match_parent">


                <ImageView
                    android:src="@drawable/hukalangupdate"
                    android:id="@+id/HukalangID"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </ImageView>


            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_margin="3dp"
                android:elevation="5dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/OliviaID"
                    android:src="@drawable/oliviaupdate"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </ImageView>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_margin="3dp"
                android:elevation="5dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/TheOliID"
                    android:src="@drawable/oli"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </ImageView>

            </androidx.cardview.widget.CardView>


        </LinearLayout>

    </RelativeLayout>


    <RelativeLayout
        android:layout_above="@+id/KKK"
        android:id="@+id/Twos"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginBottom="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="3">


            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_margin="3dp"
                android:elevation="5dp"
                android:layout_height="match_parent">

                <ImageView
                    android:src="@drawable/fuck"
                    android:id="@+id/CornerCafeID"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </ImageView>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_margin="3dp"
                android:elevation="5dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/MangobarID"
                    android:src="@drawable/mangobarupdate"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </ImageView>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_margin="3dp"
                android:layout_weight="1"
                android:elevation="5dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/SevenSessionID"
                    android:src="@drawable/sevensessionupdate"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </ImageView>

            </androidx.cardview.widget.CardView>


        </LinearLayout>

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/ButtonID"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="4">


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/Wifi"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/wifiicon" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/Wifi"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:text="Free Wifi"
                        android:textColor="@android:color/black"
                        android:textSize="12sp"
                        />

                </RelativeLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        android:id="@+id/dWifi"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/mettingevant" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/dWifi"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:text="MEETINGS"
                        android:textColor="@android:color/black"
                        android:textSize="12sp"
                        />


                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        android:id="@+id/hWifi"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/swimming_pool" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/hWifi"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:text="SWIMMING POOL"
                        android:textColor="@android:color/black"
                        android:textSize="12dp"
                        android:visibility="visible" />
                </RelativeLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/aWifi"
                        android:layout_width="35dp"
                        android:layout_centerHorizontal="true"
                        android:layout_height="35dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/exc" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/aWifi"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:text="FITNESS CENTER"
                        android:textColor="@android:color/black"
                        android:textSize="12sp"
                        android:visibility="visible" />
                </RelativeLayout>

            </LinearLayout>


        </LinearLayout>

    </RelativeLayout>


</RelativeLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/LandingNavagationID"
        android:layout_width="wrap_content"
        android:layout_gravity = "start"
        app:menu="@menu/landing_menu"
        app:headerLayout="@layout/landing_header_layout"
        android:layout_height="match_parent">

    </com.google.android.material.navigation.NavigationView>

</androidx.drawerlayout.widget.DrawerLayout>